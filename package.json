    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.3",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "typescript": "5.9.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1"
